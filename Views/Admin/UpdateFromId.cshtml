                                <i class="glyphicon glyphicon-cog"></i>
                            </a>
                            <a href="#" class="btn btn-minimize btn-round btn-default">
                                <i class="glyphicon glyphicon-chevron-up"></i>
                            </a>
                            <a href="#" class="btn btn-close btn-round btn-default">
                                <i class="glyphicon glyphicon-remove"></i>
                            </a>
                        </div>
                    </div>
                    <div class="box-content">
                        @*<form role="form" action="~/Admin/FinalUpdate">
                            <div class="form-group">

                                <label for="exampleInputEmail1"> Item Id</label>*@
                        @using (Html.BeginForm("UpdateFinaly", "Admin", FormMethod.Post))
                        {

                            @Html.HiddenFor(x => x.Id)
                            @Html.LabelFor(x => x.Name)
                            @Html.EditorFor(x => x.Name)
                            @Html.Label("Password")
                            @Html.TextAreaFor(x => x.Password)
                            
                            @Html.HiddenFor(x => x.Email)
                            @Html.Label("Contact")
                            @Html.EditorFor(x => x.Contact)
                            
                            @Html.Label("Type")
                            @Html.HiddenFor(x => x.Type)
                            
                            <button type="submit" class="btn btn-default">Update </button>
                        }



